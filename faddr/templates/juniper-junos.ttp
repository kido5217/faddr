<group>
interfaces { {{ _start_ }}
    {{ name }} {
        description {{ description | ORPHRASE }};
        native-vlan-id {{ native_vlan }};
        unit {{ unit | _start_ }} {
            description {{ description | ORPHRASE }};
            vlan-id {{ s_vlan }};
            vlan-tags outer {{ s_vlan }} inner {{ c_vlan }};
        } {{ _end_ }}
    }
} {{ _end_ }}
</group>
